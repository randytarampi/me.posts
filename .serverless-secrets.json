{
  "options": {
    "throwOnMissingSecret": true,
    "logOnMissingSecret": true,
    "skipValidation": false,
    "omitPermissions": false,
    "resourceForIamRole": "*",
    "providerOptions": {
      "region": "us-east-1"
    },
    "keys": {
      "dev": "alias/serverless-dev",
      "prd": "alias/serverless-prd"
    },
    "provider": "aws"
  },
  "environments": {
    "$global": {
      "FLICKR_API_KEY": "flickr-api-key",
      "FLICKR_API_SECRET": "flickr-api-secret",
      "UNSPLASH_API_KEY": "unsplash-api-key",
      "UNSPLASH_API_SECRET": "unsplash-api-secret",
      "INSTAGRAM_API_KEY": "instagram-api-key",
      "INSTAGRAM_API_SECRET": "instagram-api-secret",
      "INSTAGRAM_ACCESS_TOKEN": "instagram-access-token",
      "TUMBLR_API_KEY": "tumblr-api-key",
      "TUMBLR_API_SECRET": "tumblr-api-secret",
      "SENTRY_DSN": "sentry-dsn"
    }
  }
}
